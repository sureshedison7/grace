{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "types": [
        "@cloudflare/workers-types/2023-07-01"
    ],
    "baseUrl": "src",
    "jsx": "react-jsx",
    "paths": {
      "@assets/*": [
        "assets/*"
      ],
      "@config": [
        "config.ts"
      ],
      "@components/*": [
        "components/*"
      ],
      "@content/*": [
        "content/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "jsxImportSource": "react"
  }
}